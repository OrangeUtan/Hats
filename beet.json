{
    "name": "Hats",
    "author": "Oran9eUtan",
    "description": "Stylish Hats for Minecraft",
    "output": "build",
    "data_pack": {
        "name": "hats_datapack",
        "load": ["datapack"]
    },
    "resource_pack": {
        "name": "Hats",
        "load": ["resourcepack"]
    },
    "pipeline": [
        "babelbox.integration.beet",
        "mcanitexgen.integration.beet"
    ],
    "meta": {
        "mcanitexgen": {
            "load": ["resourcepack/assets/minecraft/textures/**/*.animation.py"]
        },
        "babelbox": {
            "load": ["resourcepack/assets/minecraft/lang"],
            "dialect": "excel",
            "prefix_identifiers": true
        }
    }
}
